{
  "env": {
    "browser": true,
    "es6": true
  },
  "globals": {
    "webix": true,
    "scheduler": true,
    "APPNAME": true,
    "VERSION": true,
    "PRODUCTION": true,
    "BUILD_AS_MODULE": true
  },
  "extends": ["plugin:prettier/recommended"],
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  }
}
